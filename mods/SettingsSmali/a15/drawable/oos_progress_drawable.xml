<?xml version='1.0' encoding='utf-8' ?>
<layer-list android:paddingMode="stack" xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:gravity="center|fill_horizontal"
        android:id="@android:id/background">
    <shape android:tint="?android:attr/colorAccent">
      <solid android:color="#33ffffff" />
      <size android:height="5.0dp" />
      <corners android:radius="8.0dp" />
    </shape>
  </item>
  <item android:gravity="center|fill_horizontal"
        android:id="@android:id/progress">
    <clip>
      <shape android:tint="@android:color/system_accent1_200">
        <size android:height="5.0dp" />
        <corners android:radius="8.0dp" />
      </shape>
    </clip>
  </item>
</layer-list>